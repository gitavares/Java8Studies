package com.giselle;

import java.util.Scanner;

public class X {
	private int x;

	public X(Scanner x) {
		super();
		Scanner scanner = new Scanner(System.in);
		System.out.println("Please enter a number: ");
		this.x = x.nextInt();
	}
	
	public void x(){
		for (int x = 1; x < 13; x++) {
			System.out.println(x + " times " + this.x + " equals " + x * this.x);
		}
	}
}
